CREATE TABLE cliente (
	cd_Cliente BIGINT(20) PRIMARY KEY AUTO_INCREMENT,
	nm_Cliente VARCHAR (50) NOT NULL,
	email VARCHAR (40) NULL,
	ds_Senha VARCHAR (10) NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


