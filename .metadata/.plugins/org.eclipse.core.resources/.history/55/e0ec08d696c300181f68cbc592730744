CREATE TABLE cliente (
	cd_Cliente BIGINT(20) PRIMARY KEY AUTO_INCREMENT,
	nm_Cliente VARCHAR (100) NOT NULL,
	email VARCHAR (80) NULL,
	ds_Senha VARCHAR (80) NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


