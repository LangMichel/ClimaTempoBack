package com.ddm.climatempo.api;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.boot.context.properties.EnableConfigurationProperties;

import com.ddm.salonmanager.api.config.property.SalonManagerApiProperty;

@SpringBootApplication
@EnableConfigurationProperties(SalonManagerApiProperty.class)
public class SalonmanagerApiApplication {

	public static void main(String[] args) {
		SpringApplication.run(SalonmanagerApiApplication.class, args);
	}
}
