package com.ddm.climatempo.api.repository.projection;

public class ResumoCidade {
			
		private Long cd_Cidade;
		private String nm_Cidade;
		private String nm_Pais;
		private String sg_Pais;
		
		public ResumoCidade(Long cd_Cidade, String nm_Cidade, String nm_Pais, String sg_Pais) {
			super();
			this.cd_Cidade = cd_Cidade;
			this.nm_Cidade = nm_Cidade;
			this.nm_Pais = nm_Pais;
			this.sg_Pais = sg_Pais;
		}
		
		
		public String getDs_Marca() {
			return ds_Marca;
		}


		public void setDs_Marca(String ds_Marca) {
			this.ds_Marca = ds_Marca;
		}


		public String getNm_Marca() {
			return nm_Marca;
		}


		public void setNm_Marca(String nm_Marca) {
			this.nm_Marca = nm_Marca;
		}


		public Long getCd_Marca() {
			return cd_Marca;
		}

		public void setCd_Marca(Long cd_Marca) {
			this.cd_Marca = cd_Marca;
		}
		

}
